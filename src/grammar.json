{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "kolibri",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "expression"
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_term"
        },
        {
          "type": "SYMBOL",
          "name": "operator_expression"
        },
        {
          "type": "SYMBOL",
          "name": "apply"
        },
        {
          "type": "SYMBOL",
          "name": "opcurry"
        }
      ]
    },
    "_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "parenthetical_expression"
        },
        {
          "type": "SYMBOL",
          "name": "curly_expression"
        }
      ]
    },
    "operator_expression": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "st_op"
              },
              {
                "type": "SYMBOL",
                "name": "main_op"
              },
              {
                "type": "SYMBOL",
                "name": "op"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          }
        ]
      }
    },
    "parenthetical_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "curly_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "opcurry": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "("
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "st_op"
              },
              {
                "type": "SYMBOL",
                "name": "main_op"
              },
              {
                "type": "SYMBOL",
                "name": "op"
              }
            ]
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "expression"
            }
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "apply": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_term"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_term"
            }
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\.)*\""
    },
    "st_op": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "|>"
        },
        {
          "type": "STRING",
          "value": "->"
        },
        {
          "type": "STRING",
          "value": ":>"
        },
        {
          "type": "STRING",
          "value": "!>"
        },
        {
          "type": "STRING",
          "value": "<!"
        },
        {
          "type": "STRING",
          "value": "<:"
        },
        {
          "type": "STRING",
          "value": "<-"
        },
        {
          "type": "STRING",
          "value": "<|"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "<"
        }
      ]
    },
    "main_op": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "op": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\w\\s(){}]"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [
    [
      {
        "type": "SYMBOL",
        "name": "apply"
      },
      {
        "type": "SYMBOL",
        "name": "opcurry"
      }
    ],
    [
      {
        "type": "SYMBOL",
        "name": "_term"
      }
    ]
  ],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}